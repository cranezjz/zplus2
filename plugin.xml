<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="zplus2.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="zplus2.commands.category"
            id="zplus2.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="zplus2.commands.sampleCommand"
            class="zplus2.handlers.SampleHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="zplus2.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="menu2"
               mnemonic="M"
               id="zplus2.menus.sampleMenu">
            <command
                  commandId="zplus2.commands.sampleCommand"
                  mnemonic="S"
                  id="zplus2.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="zplus2.toolbars.sampleToolbar">
            <command
                  commandId="zplus2.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="zplus2.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension point="org.eclipse.ui.console.consoleFactories">
         <consoleFactory 
	     label="zjz_Console_2"
	     class="zplus2.MyConsole"
	     icon="icons/sample.gif">
	  </consoleFactory>
   </extension>
	<extension point="org.eclipse.ui.startup">
   		<startup class="zplus2.EasyStartup"/>
   </extension>
 <extension
       point="org.eclipse.core.resources.modelProviders">
 </extension>
 <extension
       point="org.eclipse.core.resources.builders">
 </extension>
</plugin>
